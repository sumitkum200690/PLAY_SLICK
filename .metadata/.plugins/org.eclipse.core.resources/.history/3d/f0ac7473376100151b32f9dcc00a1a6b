@(empDesciprion: List[models.EmployeeModel])

		
		
			@main("EMPLOYEE DESCRIPTION") {
			
			<table>
			  
			  @for(employee <- empDesciprion) {
			
				<tr><th>EMPLOYEE ID</th> <th> &nbsp; &nbsp;&nbsp;EMPLOYEE NAME</th> </tr>
			
				<tr><td>@employee.id</td><td>&nbsp; &nbsp;&nbsp&nbsp; &nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; &nbsp;&nbsp; @employee.name</td></tr> 
				
			 }
			  </table>
			
			}
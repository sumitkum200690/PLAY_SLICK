@(empDesciprion: List[models.EmployeeModel])

		
		
			@main("EMPLOYEE DESCRIPTION") {
			
			<table>
			  
			  @for(employee <- empDesciprion) {
			
				<tr><th>EMPLOYEE ID</th> <th>EMPLOYEE NAME</th> </tr>
			
				<tr><td>@employee.id</td><td> @employee.name</td></tr> 
				
			 }
			  </table>
			
			}